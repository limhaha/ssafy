package com.ssafy.day2.method;

public class ObjectArrayTest {

	public static void main(String[] args) {
		// 클래스 배열 
		
		int[] arr = new int[5];
		
		SPerson [] brr = new SPerson[5]; //객체 배열
		
		System.out.println(arr[2]);
		System.out.println(brr[2]);
	}

}
class SPerson{
	String name;
	int age;
}
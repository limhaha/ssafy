package com.ssafy.day2.method;

public class CallTest {

	public static void main(String[] args) {
		int n = 10;
		System.out.println("n1 : " + n);
		m1(n);
		System.out.println("n2 : " + n);
		
	}
	
	static void m1(int n) {
		n = n + 10;
		System.out.println("m1 n : " + n);
	}
	
	static void m2(S s) {
		
	}
}


class S{
	int a;
}
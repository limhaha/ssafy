package com.ssafy.algo;

import java.util.Scanner;

public class Solution22 {

	public static void main(String[] args) {
		int T, N, num;
		
		Scanner sc = new Scanner(System.in);
		T = sc.nextInt();
		for(int testcase=0; testcase<T; testcase++) {
			N = sc.nextInt();
			num = sc.nextInt();
			
			int[][] map = new int[N][N];
			
//			for(int i=0; i<num; i++) {
//				
//			}
			
			for(int[] a : map) {
				for(int b : a) {
					System.out.println(b);
				}
			}
		}

	}

}

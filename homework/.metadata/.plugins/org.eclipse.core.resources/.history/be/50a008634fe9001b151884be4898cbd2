package com.ssafy.algo;

import java.util.Scanner;

public class DigitTest1 {

	public static void main(String[] args) {
		int[] ints = new int[10];
		
		Scanner sc = new Scanner(System.in);
		
		while(true) {
			int num = sc.nextInt();
			if(num == 0) {
				break;
			}
			ints[num/10]++;
		}
		
		for(int i=0; i<ints.length; i++) {
			if(ints[i] != 0) {
			System.out.println(i + " : " + ints[i] + "ê°œ");
			}
		}
		
	}

}

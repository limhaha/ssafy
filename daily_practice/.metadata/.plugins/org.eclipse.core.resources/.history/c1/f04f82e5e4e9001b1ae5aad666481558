package com.ssafy.ws02.step3;

import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {
		int T;
		
		Scanner sc = new Scanner(System.in);
		T = sc.nextInt();
		
		for(int i=0; i<T; i++) {
			int N = sc.nextInt();
			char[][] map = new char[N][N];
			
			for(int j=0; j<N; j++) {
				for(int k=0; k<N; k++) {
					map[j][k] = sc.next().charAt(0); 
				}
			}
			
			for(int j=0; j<N; j++) {
				for(int k=0; k<N; k++) {
					System.out.println(map[j][k]);
				}
			}
		}
		
	

	}

}
